{
  "database": {
    "path": "admin.db"
  },
  "policy": {
    "file": "policy.yaml"
  },
  "auth": {
    "username_env": "ADMIN_USERNAME",
    "password_env": "ADMIN_PASSWORD"
  },
  "grpc": {
    "port": 50052,
    "tls": {
      "cert": "",
      "key": "",
      "require": false
    }
  },
  "web": {
    "port": 8080,
    "host": "127.0.0.1",
    "secret_key_env": "ADMIN_SECRET_KEY",
    "cookie_secure": false,
    "tls": {
      "cert": "",
      "key": "",
      "require": false
    }
  },
  "siem": {
    "elasticsearch": {
      "enabled": false,
      "url": "http://localhost:9200"
    },
    "syslog": {
      "enabled": true
    },
    "webhook": {
      "enabled": false,
      "url": ""
    }
  }
}